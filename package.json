{
  "name": "directives",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "generate:schema": "graphql get-schema -p database",
    "generate:typings": "graphql codegen -p database",
    "generate": "npm run -p -l generate:schema generate:typings",
    "generate:watch": "nodemon -e graphql --exec 'npm run generate'",
    "deploy:generate": "run-s -l deploy generate:watch",
    "deploy": "cd database && prisma deploy",
    "dev": "run-p -l generate:watch start playground",
    "start": "ts-node src/index.ts",
    "debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
    "playground": "graphql playground",
    "build": "rimraf dist && tsc"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "express-session": "^1.15.6",
    "graphql": "^14.0.2",
    "graphql-yoga": "1.16.2",
    "merge-graphql-schemas": "^1.5.7",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/express-session": "^1.15.11",
    "@types/passport-jwt": "^3.0.1",
    "dotenv-cli": "1.4.0",
    "graphql-binding": "^2.2.5",
    "graphql-cli": "2.16.7",
    "graphql-static-binding": "^0.9.3",
    "graphql-tools": "^4.0.0",
    "nodemon": "1.18.4",
    "npm-run-all": "4.1.3",
    "prisma": "1.17.1",
    "prisma-binding": "^2.1.6",
    "ts-node": "7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1"
  }
}
